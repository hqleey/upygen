module main {
  var n: integer;
  var a: integer;
  var b: integer;
  var c: integer;
  init {
    n = 1;
    a = 0;
    b = 1;
    c = 0;
  }
  next {
    if (n > 0) {
      c' = a + b;
      a' = b;
      b' = c;
      n' = n - 1;
    }
    else {
      n' = 0;
    }
  }
  control{
    bmc(5);

  }
}